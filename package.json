{
  "name": "easy-mediasoup",
  "version": "1.0.3",
  "description": "Mediasoup overhead",
  "main": "./build/main.bundle.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm test -- -w",
    "init": "mkdir es5-bundle",
    "clean": "rm -rf es5-bundle",
    "prebuild": "npm run clean && npm run init",
    "build": "babel ./src -d ./es5-bundle --ignore test.js --presets babel-preset-es2015",
    "pretest": "npm run build",
    "test": "mocha --compilers js:babel-core/register ./src/**/*.test.js",
    "test:single": "mocha --compilers js:babel-core/register",
    "bundle_build": "npm run clean && npm run build && node build.js",
    "test-page": "echo \"open https://0.0.0.0:8443/test/\" && http-server",
    "lint": "jshint .",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Adrinalin4ik/easy-mediasoup.git"
  },
  "keywords": [
    "mediasoup",
    "webrtc",
    "sfu"
  ],
  "author": "Alexey Panfilkin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Adrinalin4ik/easy-mediasoup/issues"
  },
  "homepage": "https://github.com/Adrinalin4ik/easy-mediasoup#readme",
  "dependencies": {
    "babel-preset-stage-2": "^6.24.1",
    "debug": "^3.1.0",
    "mediasoup-client": "^2.0.3",
    "protoo-client": "^2.0.5",
    "random-string": "^0.2.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "url-parse": "^1.1.9",
    "webrtc-adapter": "^4.0.0",
    "wildemitter": "^1.2.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "chromedriver": "^2.29.0",
    "compression-webpack-plugin": "^1.1.3",
    "geckodriver": "^1.6.1",
    "http-server": "^0.2.2",
    "jshint": "^2.9.5",
    "jshint-loader": "^0.8.4",
    "mocha": "^3.1.2",
    "node-libs-browser": "^2.1.0",
    "precommit-hook": "^3.0.0",
    "request": "^2.72.0",
    "selenium-webdriver": "^3.0.1",
    "tape": "^4.0.0",
    "travis-multirunner": "^3.0.0",
    "uglify-js": "^2.7.3",
    "webpack": "^3.10.0"
  },
  "files": [
    "dist"
  ]
}
